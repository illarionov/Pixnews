{
  "markdown": {
      "lineWidth": 120,
      "emphasisKind": "asterisks"
  },
  "toml": {
      "indentWidth": 4
  },
  "includes": ["**/*.{md,toml}"],
  "excludes": [
    "**/node_modules",
    "**/build/",
    "**/*-lock.json",
    "gradle/plugin/fbase-options-gradle-plugin",
    "gradle/plugin/base/prefiller/"
  ],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.15.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm"
  ]
}
